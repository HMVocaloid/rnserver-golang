    <script language="javascript">
    	var pv = 0;
    	var undefined;

	if (~navigator.userAgent.indexOf('MSIE') && !~navigator.userAgent.indexOf('Mac')) {
		vers = IERPCtl.RealPlayerVersion;
		if (undefined != vers) {
			player_major = ((vers) >> 28);
			player_minor = ((vers & 0xFF00000) >> 20);
			pv = ((vers & 0xFF000) >> 12);
			player_build = ((vers & 0xFFF));
		} else {
			realplugins = [
				"RealVideo.RealVideo(tm) ActiveX Control (32-bit)",
				"RealPlayer.RealPlayer(tm) ActiveX Control (32-bit)",
				"rmocx.RealPlayer G2 Control"
			];
			try {new ActiveXObject(realplugins[0]); pv = 4} catch (e) {}
			try {new ActiveXObject(realplugins[1]); pv = 5} catch (e) {}
			try {new ActiveXObject(realplugins[2]); pv = 6} catch (e) {}
		}
	} else {
		if ( navigator.plugins.length > 0 && navigator.plugins['RealOne Player Version Plugin']) {
			vers = navigator.plugins['RealOne Player Version Plugin'].description.split(".");
			player_major = parseInt(vers[0]);
			player_minor = parseInt(vers[1]);
			pv = parseInt(vers[2]);
			player_build = parseInt(vers[3]);
		} else {
			realplugins = [
				"RealVideo",
				"RealPlayer",
				"RealPlayer G2"
			];
			for (i = 0; i < navigator.plugins.length; ++i) {
				if (~navigator.plugins[i].name.indexOf(realplugins[0])) pv = 4;
				if (~navigator.plugins[i].name.indexOf(realplugins[1])) pv = 5;
				if (~navigator.plugins[i].name.indexOf(realplugins[2])) pv = 6;
			}
		}
	}
    </script>


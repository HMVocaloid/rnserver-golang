<script language="JavaScript">
	
	// pop a windoid (Pictures)
	function popWin(url, w, h, winname) {
		var madURL = url;
		var x, y, winStr;
		
		x=560; y=0;
	
		winStr = "height="+h+",width="+w+",screenX="+x+",left="+x+",screenY="+y+",top="+y+",channelmode=0,directories=0,fullscreen=0,location=0,menubar=0,resizable=1,scrollbars=1,status=0,toolbar=0";
		
		msgWindow = window.open(madURL, winname, winStr);
		if (typeof (msgWindow.focus) != "undefined") 
		{
			msgWindow.focus();
		}
	}
	// pop a windoid (Pictures)
	function popWin2(url, w, h) {
		var madURL = url;
		var x, y, winStr;
		
		x=560; y=0;
	
		self.name="opener";
		winStr = "height="+h+",width="+w+",screenX="+x+",left="+x+",screenY="+y+",top="+y+",channelmode=0,dependent=0,directories=0,fullscreen=0,location=0,menubar=0,resizable=0,scrollbars=0,status=0,toolbar=0";
		
		lilBaby = window.open(madURL, "_blank", winStr);
	}
	
	function addCount()
	{
		document.pntapplet.addCount(1);
	}

	

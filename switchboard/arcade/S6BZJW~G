//initialize detector flag
var bHLink = false;

//detect browser and assign event listener
function Init() {

if (arcadeInstalled == 1){
	// no cookie

	} else {
		if (document.attachEvent) {	//IE
		  document.attachEvent('onclick', clickHandler);
		}
		else if (document.addEventListener) {	//NN
		  document.addEventListener('click', clickHandler, false);
		}
	}
}


//onClick event handler
function clickHandler (evt) {
	if (document.attachEvent) {
		if (window.event.srcElement.tagName == "A") {
			bHLink = true;
		}  else {
			var el = window.event.srcElement;
			while ((el != null) && (el.tagName != "A") && (el.tagName !="HTML")) {
				el = el.parentElement
				if (el.tagName == "A") {
					bHLink = true;
				} else {
					bHLink = false;
				}
			}
		}
	} else if (document.addEventListener) {
		if (evt.target.parentNode.nodeName == "A" || evt.target.nodeName == "A") {
			bHLink = true;
		} else {
			bHlink = false;
		}
	}	
}

//check cookie for detector expiration
function bCheckCookie() {
	var exp = new Date();
	exp.setTime(exp.getTime() + (24*60*60*1000));

	iDef = document.cookie.indexOf("def");
	if (iDef >=0) {
		//detector is off
		return false;
	} else {
		//turn detector off for 24 hours
		document.cookie = "def=true; expires=" + exp.toGMTString();
		return true;
	}
}

//def detector action code
function DefDetect() {

if (arcadeInstalled == 1){
	//no cookie

	} else {

	if (bHLink == false && bCheckCookie()) {
		//ignore for NN 4.7

	if (navigator.appName=="Netscape" && (parseFloat(navigator.appVersion) <= 4.8)) {
			// do nothing
		} else {
			//spawn window
				window.open("popup_030818.html?src=rahome030312_1&mode=spnav&distcode=R140&tps=rahome-2_","winMessage","scrollbars=no,left=20,top=20,width=250,height=300,noresize");
		}
	 }
	}
}

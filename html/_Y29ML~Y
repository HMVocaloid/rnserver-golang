function newWin(thisurl, name, width, height, options)
{
if (options == null || options == '')
{
options = "scrollbars=1";
}
window.open(thisurl, name,"height=" + height + ",width=" + width + ",top=60,left=60," + options);
}
function cmpPopupNamedOptions(url,name,width,height,options,target) {
if (target) target.location.href = url;
else {
newwindow = window.open(url,name,"width="+width+",height="+height+","+options+",top=60,left=60");
newwindow.focus();
}
}
</script>

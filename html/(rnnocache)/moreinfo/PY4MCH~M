<script language="JavaScript">
var popFlag = true;

function onAction(shutoff) {
//
	if (shutoff == "goAhead") popFlag = false;
	if (!(window.event && window.event.shiftKey)
		&& (!popFlag || ~document.cookie.indexOf("PNcookie"))) return;
	document.cookie = "PNcookie=yes;expires=" +new Date("5 June 2009 8:00:00").toGMTString();
	popup = window.open("/RI/RC.072904info_install_1_0_0_0_0_0.info...RI//popups/pop_.html?src=" +pagesrc_popvars, "openpopup", "width=256,height=330,left=0,top=50,resizable=no,scrollbars=no");
	popup.focus();
	setTimeout("popup.focus();", 500);
//
}

function ddAction(shutoff) {
//
	if (shutoff == "goAhead") popFlag = false;
	if ((window.event && window.event.shiftKey)
		|| (!popFlag || ~document.cookie.indexOf("seenHelperWindow=yes"))) return;
	popunder = window.open("http://ad.doubleclick.net/ad/real.guidesponsor/popunder/exit;sz=500x300;ord=5944;product=playerplus;system=windows;player=none", "Ad", "width=720,height=305,toolbar=0,location=0,directories=0,status=0,menubar=0,scrollbars=1,resizable=1");
	window.focus();
	setTimeout("window.focus();", 500);
//
}

function errorHandler() {
	var results;
	return true;
}
window.onerror = errorHandler;

function HyperWarp(url, type, target) {
	href = (eval(url||'upsell_link')).replace(".ecomm.", ".ecomm." + (type||""));
	if (!(target||window).HyperWarp) return window.open(href);
	(target||window).popFlag = false;
	(target||window).location.href = href;
}

function HyperWarp2(url){
	switch (url) {
		case "million_subs" :
			popFlag = false;
			href = "http://www.realnetworks.com/company/press/releases/2003/q103results.html";
			break;
		default :
			popFlag = false;
			href = "/RI/RC.072904info_install_1_0_0_0_0_0.info.fla..RI//realone/plus/index.html?src=072904info_install_1_0_0_0_0_0";
			break;
	}
	location.href = href;
}

function cmpPopupNamedOptions(url,name,width,height,options,target) {
	if (target) target.location.href = url;
	else {
		newwindow = window.open(url,name,"width="+width+",height="+height+","+options+",top=60,left=60");
		newwindow.focus();
	}
}

</script>




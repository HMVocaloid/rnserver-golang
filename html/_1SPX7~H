document.onclick = function (e) {
if (!e) e = window.event;
if (!e.target) e.target = e.srcElement;
var anchor, target;
for (target = e.target; target.parentNode; target = target.parentNode) {
if (!anchor && target.href && !target.src) anchor = new Object(target);
if (~target.id.indexOf("div") || ~target.id.indexOf("stage") ||
~target.id.indexOf("header") || ~target.id.indexOf("footer")) break;
}
var A = target.getElementsByTagName("a");

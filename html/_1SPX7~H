<script language="Javascript">
document.cookie = "PNDcookie=yes"
+ ";expires=" +new Date("3 Mar 2009 17:49:29").toGMTString();
showDD=true;
window.onunload = unloadFunc;
document.body.onclick= supressDD;
function supressDD(){
showDD=false;
}

<script>


function loadStyle()
{
	if( (navigator.appName.indexOf("Netscape") >= 0)&&(parseInt(navigator.appVersion))<5 )
	{
		document.write("<LINK REL=STYLESHEET TYPE=\"text/css\" HREF=\"/styles/netscape_style_.css\">");
	}else{
		document.write("<LINK REL=STYLESHEET TYPE=\"text/css\" HREF=\"/styles/ie_style_.css\">");
	}
}

loadStyle();
</script>
